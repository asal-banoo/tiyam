            @Html.DisplayFor(modelItem => item.PriceBoon)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BankGetNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BankTransNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PostBarCode)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
